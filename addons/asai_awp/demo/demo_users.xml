<odoo>
    <!-- Добавляю суперопльзователя в группу админов -->
    <record id="base.user_admin" model="res.users">
        <field name="groups_id" eval="[(4, ref('asai_awp.group_awp_admin'))]"/>
    </record>

    <!-- Создаю демо- рядового пользователя 1 -->
    <record id="user1" model="res.users">
        <field name="name">user1</field>
        <field name="login">user1</field>
        <field name="password">123</field>
        <field name="groups_id" eval="[(4, ref('base.group_user')), (4, ref('asai_awp.group_awp_user'))]"/>
    </record>

    <!-- Создаю демо- рядового пользователя 2 -->
    <record id="user2" model="res.users">
        <field name="name">user2</field>
        <field name="login">user2</field>
        <field name="password">123</field>
        <field name="groups_id" eval="[(4, ref('base.group_user')), (4, ref('asai_awp.group_awp_user'))]"/>
    </record>

    <!-- Создаю манагера -->
    <record id="manager" model="res.users">
        <field name="name">manager</field>
        <field name="login">manager</field>
        <field name="password">123</field>
        <field name="groups_id" eval="[(4, ref('base.group_user')), (4, ref('asai_awp.group_awp_manager'))]"/>
    </record>

    <!-- Создаю админа (не суперпользователя!) -->
    <record id="user_admin" model="res.users">
        <field name="name">admin</field>
        <field name="login">admin</field>
        <field name="password">123</field>
        <field name="groups_id" eval="[(4, ref('base.group_user')), (4, ref('asai_awp.group_awp_admin'))]"/>
    </record>
</odoo>